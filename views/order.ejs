  <main>

    <div class="container py-3">
      <div class="row">
        <div class="col">
          <form id="order-form" method="POST" action="/order">

            <div class="form-group">
              <label for="inputFirstName validationDefault01">First Name</label>
              <input type="text" name="inputFirstName" class="form-control" id="inputFirstName validationDefault01" required>
            </div>
            <div class="form-group">
              <label for="inputLastName validationDefault02">Last Name</label>
              <input type="text" name="inputLastName" class="form-control" id="inputLastName validationDefault02" required>
            </div>
            <div class="form-group">
              <label for="inputEmail validationDefault03">Email</label>
              <input type="text" name="inputEmail" class="form-control" id="inputEmail validationDefault03" required>
            </div>
            <div class="form-group">
              <label for="inputPhone validationDefault04">Phone</label>
              <input type="text" name="inputPhone" class="form-control" id="inputPhone validationDefault04" required>
            </div>
            <div class="form-group">
              <label for="inputAddress validationDefault05">Address</label>
              <input type="text" name="inputAddress" class="form-control" id="inputAddress validationDefault05" required>
            </div>
            <div class="form-row">
              <div class="form-group col-md-4">
                <label for="inputCity validationDefault06">City</label>
                <input type="text" name="inputCity" class="form-control" id="inputCity validationDefault06" required>
              </div>
              <div class="form-group col-md-4">
                <label for="inputProvince validationDefault07">Province</label>
                <input type="text" name="inputProvince" id="inputProvince validationDefault07" class="form-control" required>
              </div>
              <div class="form-group col-md-4">
                <label for="inputPostalCode validationDefault08">Postal Code</label>
                <input type="text" name="inputPostalCode" class="form-control" id="inputPostalCode validationDefault08" required>
              </div>
            </div>
            <div class="form-group">
              <label for="inputTextarea">Additional Order Details</label>
              <textarea class="form-control" name="inputTextarea" id="inputTextarea" rows="3"></textarea>
            </div>
            <input type="submit" value="Place your order" class="btn btn-primary"></input>
          </form>
        </div>

